{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "*.svg", "*.png", "*.jpg", "*.jpeg", "*.gif", "*.ico", "*.css", "*.js"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
